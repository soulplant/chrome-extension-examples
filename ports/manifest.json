{
  "name": "Port Test",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
    "webNavigation",
    "http://*/*",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["style.css"],
      "js": ["script.js"]
    }
  ]
}
