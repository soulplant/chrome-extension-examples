{
  "name": "lazy browser action test",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "webNavigation",
    "storage",
    "http://*/*",
    "tabs"
  ]
}
